
public class Tokenizer {
	static Vector<TokenDef> definitions;
	
	public static class Token {
		public int type;
		public String val;
	}
	
	public static class TokenDef {
		public String regex;
		public int type;
		public int precedence;
	}
	
	public static class TokenMatch {
		public int type;
		public String val;
		public int start, end;
		public int precedence;
	}
}
